{
  "productName": "NCTS Syndication",
  "version": "0.1.0",
  "identifier": "com.ncts.syndication",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "frontendDist": "ui"
  },
  "bundle": {
    "active": false,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.png"
    ]
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "NCTS Syndication",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": "default-src 'self'; script-src 'self'; style-src 'self'; connect-src 'self' ipc://localhost https://api.healthterminologies.gov.au https://tx.fhir.org"
    }
  }
}
